   "source": [
    "from PyQt6.QtWidgets import *\n",
    "\n",
    "\n",
    "app = QApplication([])\n",
    "btn = QPushButton('Click')\n",
    "\n",
    "def onbuttonClick():\n",
    "    alert = QMessageBox()\n",
    "    alert.setText('Button Clicked')\n",
    "    alert.exec()\n",
    "    \n",
    "btn.clicked.connect(onbuttonClick)    \n",
    "btn.show()\n",
    "app.exec()\n",
    "\n"
   ]
